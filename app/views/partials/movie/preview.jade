div.movie-thumbnail
  | <img width="55" height="75" src="<%= movie.posters.thumbnail%>" />
div.movie-details
  div.movie-title
    span <%= movie.title %>
  div.movie-subdetails
    div.movie-year
      label Released:&nbsp;
      em <%= movie.year %>
    div.movie-rating
      label User Rating:&nbsp;
      span.rating-text <%= movie.ratings.audience_score %>%


